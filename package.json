{
  "name": "objection-graphql",
  "version": "0.1.0",
  "description": "Automatically generates GraphQL schema for objection.js models",
  "main": "objection-graphql.js",
  "scripts": {
    "test": "istanbul --config=.istanbul.yml cover _mocha -- --slow 10 --timeout 5000 --reporter spec tests",
    "test-only": "mocha --slow 10 --timeout 5000 --reporter spec --recursive tests",
    "test-bail": "mocha --slow 10 --timeout 5000 --reporter spec --recursive tests --bail"
  },
  "keywords": [
    "GraphQL",
    "SQL",
    "objection.js"
  ],
  "author": "Sami Koskim√§ki <sami.koskimaki@vincit.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/vincit/objection-graphql.js.git"
  },
  "files": [
    "README.md",
    "LICENSE",
    "lib/*",
    "objection-graphql.js"
  ],
  "peerDependencies": {
    "graphql": "^0.7.0",
    "objection": "> 0.5.0"
  },
  "dependencies": {
    "lodash": "^4.15.0"
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "graphql": "^0.7.0",
    "istanbul": "^0.4.5",
    "knex": "^0.11.10",
    "mocha": "^3.0.2",
    "objection": "next",
    "sqlite3": "^3.1.4"
  }
}
